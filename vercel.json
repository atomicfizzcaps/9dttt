{
  "version": 2,
  "name": "9dttt-frontend",
  "public": true,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "Public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/Public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ninedttt.onrender.com/api/:path*"
    },
    {
      "source": "/socket.io/:path*",
      "destination": "https://ninedttt.onrender.com/socket.io/:path*"
    }
  ]
}
